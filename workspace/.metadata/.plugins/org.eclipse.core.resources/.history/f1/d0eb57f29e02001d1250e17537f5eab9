package com.globallogic.helloworld;

public class MyLadder {

	public static void main(String[] args) {
		
		int num=1;
		
		if(num==1)
		{
			System.out.println("Sun");
		}
		else if (num==2)
		{
			System.out.println("Mon");
		}
		else if(num==3)
		{
			System.out.println("Tue");
		}
		else
		{
			System.out.println("Wrong input");
		}
		
	}
}
